<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="group26.cs307.allinwallet">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SignupActivity"
            android:label="@string/title_activity_signup" />
        <activity
            android:name=".LoginActivity"
            android:label="@string/title_activity_login" />
        <activity
            android:name=".MainPage"
            android:label="@string/title_activity_main_page" />
        <activity
            android:name=".ResetPasswordActivity"
            android:label="@string/title_activity_reset_password" />
        <activity
            android:name=".AddPurchase"
            android:label="@string/title_activity_add_purchase" />
        <activity
            android:name=".Profile"
            android:label="@string/title_activity_profile" />
        <activity android:name=".Report" />
        <activity
            android:name=".Search_by_name"
            android:label="@string/title_activity_search_by_name" />
        <activity
            android:name=".Search_by_category"
            android:label="@string/title_activity_search_by_category"></activity>
    </application>

</manifest>